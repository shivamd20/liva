{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "liva",
	"main": "server/index.ts",
	"compatibility_date": "2025-11-28",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"migrations": [
		{
			"new_sqlite_classes": [
				"MyDurableObject"
			],
			"tag": "v1"
		},
		{
			"new_sqlite_classes": [
				"NoteDurableObject",
				"NoteIndexDurableObject"
			],
			"tag": "v2"
		},
		{
			"new_sqlite_classes": [
				"ConversationDurableObject"
			],
			"tag": "v3"
		},
		{
			"new_sqlite_classes": [
				"RecordingDurableObject"
			],
			"tag": "v4"
		},
		{
			"new_sqlite_classes": [
				"MonorailSessionDO"
			],
			"tag": "v5"
		},
		{
			"new_sqlite_classes": [
				"YouTubeIntegrationDO"
			],
			"tag": "v6"
		},
		{
			"new_sqlite_classes": [
				"YouTubePublishSessionDO"
			],
			"tag": "v7"
		},
		{
			"new_sqlite_classes": [
				"VideosDO"
			],
			"tag": "v8"
		}
	],
	"assets": {
		"directory": "./public",
		"not_found_handling": "single-page-application",
		"run_worker_first": [
			"/api/*",
			"/ws/note/*"
		]
	},
	"durable_objects": {
		"bindings": [
			{
				"class_name": "MyDurableObject",
				"name": "MY_DURABLE_OBJECT"
			},
			{
				"class_name": "NoteDurableObject",
				"name": "NOTE_DURABLE_OBJECT"
			},
			{
				"class_name": "NoteIndexDurableObject",
				"name": "NOTE_INDEX_DURABLE_OBJECT"
			},
			{
				"class_name": "ConversationDurableObject",
				"name": "CONVERSATION_DURABLE_OBJECT"
			},
			{
				"class_name": "RecordingDurableObject",
				"name": "RECORDING_DURABLE_OBJECT"
			},
			{
				"class_name": "MonorailSessionDO",
				"name": "MONORAIL_SESSION_DO"
			},
			{
				"class_name": "YouTubeIntegrationDO",
				"name": "YOUTUBE_INTEGRATION_DO"
			},
			{
				"class_name": "YouTubePublishSessionDO",
				"name": "YOUTUBE_PUBLISH_SESSION_DO"
			},
			{
				"class_name": "VideosDO",
				"name": "VIDEOS_DO"
			}
		]
	},
	"r2_buckets": [
		{
			"binding": "files",
			"bucket_name": "din-files"
		}
	],
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "liva_db",
			"database_name": "liva-db",
			"database_id": "afc565c7-a861-4c2a-9686-40a905c483a4",
			"remote": false
		}
	],
	"ai": {
		"binding": "AI"
	},
	"vars": {
		"BETTER_AUTH_SECRET": "CHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRETCHANGE_ME_TO_A_RANDOM_SECRET",
		"AUTH_BASE_URL": "https://liva.shvm.in",
		"GOOGLE_CLIENT_ID": "590117253094-s8kso38hd3843apbs68gjei368ccrojp.apps.googleusercontent.com",
		"VITE_CLARITY_PROJECT_ID": "ugfupp7ocg",
		"VITE_MIXPANEL_TOKEN": "ba88a4da68ad14e815b9a5195b6a6dcf"
	}
	/**
