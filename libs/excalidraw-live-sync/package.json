{
    "name": "@shvm/excalidraw-live-sync",
    "version": "0.0.3",
    "type": "module",
    "description": "Standalone library for Excalidraw live sync",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "@excalidraw/excalidraw": ">=0.17.0"
    },
    "devDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "@excalidraw/excalidraw": "^0.17.6",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "typescript": "^5.0.0",
        "vite": "^7.2.7",
        "@vitejs/plugin-react": "^4.0.0",
        "vite-plugin-dts": "^3.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}